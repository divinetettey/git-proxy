{
  "name": "@finos/git-proxy",
  "version": "2.0.0-alpha",
  "description": "Deploy custom push protections and policies on top of Git.",
  "author": "Paul Groves",
  "license": "Apache-2.0",
  "scripts": {
    "cli": "node ./packages/git-proxy-cli/index.js",
    "start": "node index.mjs",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "test:coverage": "nyc npm run test",
    "prepare": "node ./scripts/prepare.js",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" \"test/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\" \"test/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} test/**/*.{js,jsx,ts,tsx,json} --config ./.prettierrc"
  },
  "private": true,
  "packageManager": "yarn@4.1.0",
  "bin": {
    "git-proxy": "./index.mjs",
    "git-proxy-all": "concurrently 'npm run server' 'npm run client'"
  },
  "workspaces": [
    "./packages/git-proxy-cli"
  ],
  "engines": {
    "node": "20"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "plugins/*"
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "4.11.3",
    "@primer/octicons-react": "^19.8.0",
    "@seald-io/nedb": "^4.0.2",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "bit-mask": "^1.0.2",
    "body-parser": "^1.20.1",
    "chai-http": "^4.3.0",
    "classnames": "2.5.1",
    "concurrently": "^8.0.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "diff2html": "^3.4.33",
    "express": "^4.18.2",
    "express-http-proxy": "^2.0.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.1",
    "history": "5.3.0",
    "jsonschema": "^1.4.1",
    "load-plugin": "^6.0.0",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "moment": "^2.29.4",
    "mongodb": "^5.0.0",
    "nodemailer": "^6.6.1",
    "passport": "^0.7.0",
    "passport-activedirectory": "^1.0.4",
    "passport-local": "^1.0.0",
    "perfect-scrollbar": "^1.5.5",
    "prop-types": "15.8.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "6.22.3",
    "uuid": "^9.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/chai": "^4.3.11",
    "@types/eslint": "^8",
    "@types/jsdom-global": "^3",
    "@types/mocha": "^10",
    "@types/node": "^20.12.4",
    "@types/source-map-support": "^0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.10",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
